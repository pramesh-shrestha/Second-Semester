<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="296.0" prefWidth="604.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Assignment1.view.VinylController">
   <children>
      <ListView fx:id="listView" layoutX="10.0" layoutY="41.0" prefHeight="238.0" prefWidth="197.0" />
      <Label layoutX="62.0" layoutY="17.0" text="List of Vinyl">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TableView fx:id="descriptionTable" layoutX="218.0" layoutY="41.0" prefHeight="175.0" prefWidth="376.0">
        <columns>
          <TableColumn fx:id="artist" prefWidth="190.0" text="Artist" />
          <TableColumn fx:id="year" prefWidth="75.0" text="Year" />
            <TableColumn fx:id="state" prefWidth="110.0" text="State" />
        </columns>
      </TableView>
      <Button layoutX="225.0" layoutY="252.0" mnemonicParsing="false" onAction="#onReserve" text="Reserve" />
      <Button layoutX="302.0" layoutY="252.0" mnemonicParsing="false" onAction="#onBorrow" prefHeight="25.0" prefWidth="58.0" text="Borrow" />
      <Button layoutX="524.0" layoutY="252.0" mnemonicParsing="false" onAction="#onAddVinyl" text="Add Vinyl" />
   </children>
</AnchorPane>
